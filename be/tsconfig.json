{
  "compileOnSave": true,
  "compilerOptions": {
    "skipLibCheck": true,
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ],
    },
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "watch": false
  },
  "include": [
    "src/**/*",
    "initStucture.ts"
  ],
}